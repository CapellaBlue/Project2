<!DOCTYPE html>
<html>
    <head>
        <%include ../../partials/head.ejs%>
    </head>
    <body>
        <header class="header-other">
            <%include ../../partials/header.ejs%>
        </header>
        <h2 id="story-show-headline">List of Stories</h2>
        <main>
			<ul>
					<li>
                        <% for(var i = 0; i < stories.length; i++){ %>
						<a href="/stories/<%=stories[i]._id%>"><%=stories[i].title%></a><br/>
                        by: <a href="/users/<%=users._id%>"><%=stories[i].username%></a><br/>

					</li>
				<% } %>
			</ul>
		</main>
    </body>
</html>
